{
    "type": "module",
    "name": "websocket-chat",
    "description": "A tiny live chat app using Svelte & socket.io",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "dev:server": "env NODE_ENV=development nodemon -w server server",
        "start": "node server"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "nodemon": "^2.0.12",
        "prettier": "^2.4.0",
        "prettier-plugin-svelte": "^2.4.0",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.49.0"
    },
    "dependencies": {
        "express": "^4.17.1",
        "redis": "^3.1.2",
        "sirv": "^1.0.17",
        "sirv-cli": "^1.0.0",
        "socket.io": "^4.2.0",
        "socket.io-client": "^4.2.0"
    }
}
